<form id="cadastro" method="post" action="cadLogin">
    <p>Primeiro nome:</p>
    <input type="text" name="primeiroNome">    
    <p>E-mail:</p>
    <input type="text" name="email">
    <h3>Tipo de usuário</h3>
    <input type="radio" name="nivel" value="1" checked> Professor<br>
    <input type="radio" name="nivel" value="2"> Aluno<br>
    <p>Senha:</p>
    <input type="password" name="senha"><br><br>
    <input type="submit" value="Salvar">
</form>

